{
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "declarations",
    "lib": ["es6", "webworker"],
    "module": "CommonJS",
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "target",
    "test"
  ]
}